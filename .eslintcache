Overlay: {\n    width: '300px',\n    height: '450px',\n    borderRadius: '10px',\n    backgroundImage: 'linear-gradient(to bottom, rgba(0,0,0,0)0%, rgb(0,0,0,0.8)100%)',\n    paddingLeft: \"10px\",\n    paddingRight: \"10px\",\n  },\n  recipyLink: {\n    '&:hover': {\n          textDecoration: 'none !important'\n      }\n  },\n  recipyTitle: {\n      position: 'relative',\n      top: '415px',\n      left: '10px',\n      color: 'white',\n      fontWeight: 'bold',\n  },\n  scroll: {\n    position: \"relative\",\n    top: \"-40px\",\n    right: \"15px\",\n    textAlign: \"right\",\n    cursor: \"pointer\",\n    color: BLACK_BUTTON_PRIMARY,\n    '&:hover svg': {\n      transform: \"translateX(15px)\"\n    },\n    '& svg': {\n      transition: \"transform 450ms\",\n    }\n  }\n}));\n\n\nfunction RecetaRow({category, id, classes}) {\n    const [recipes, setRecipes] = useState([]);\n    const [{},dispatch] = useDataLayerValue();\n    classes = useStyles();\n    \n    useEffect(() => {\n        firebase.db.collection(\"recetas\").doc(id).collection(\"listaRecetas\").onSnapshot(snapshot => {\n            const rec = []; // recipes\n            snapshot.docs.forEach(doc => {\n                const data = {\n                    titulo: doc.data().titulo,\n                    image: doc.data().image,\n                    id: doc.id\n                }\n                rec.push(data);\n            })\n            setRecipes(rec);\n        })\n    },[id])\n\n    const setRecipy = (recipyId) => {\n        dispatch({\n            type: \"SET_CATEGORY_ID\",\n            categoryId: id,\n        })\n        dispatch({\n            type: \"SET_CATEGORY_TITLE\",\n            categoryTitle: category,\n        })\n        dispatch({\n            type: \"SET_RECIPY_ID\",\n            recipyId: recipyId,\n        })\n    }\n\n    const calculateLength = (title) => {\n        return (450 - 32 * (Math.floor(title.length / 22)+1))\n    }\n\n    return (\n        <>\n            {recipes.length > 0 &&\n                <div style={{marginBottom: '50px'}}>\n                <h2 style={{position: 'relative', color: BLACK_BUTTON_PRIMARY,  textAlign: \"left\", fontWeight: 'bold', left: '20px'}}>{category}</h2>\n                <div className={classes.recipesContainer}>\n                    {recipes && recipes.map((recipy, i) => (\n                        <Link key={i} to=\"/receta-post\" className={classes.recipyLink} onClick={() => setRecipy(recipy.id)}>\n                            <div \n                                className={classes.recipeContainer}\n                                style={{backgroundImage: `url(${recipy.image})`}}\n                                >\n                                    <div className={classes.recipyOverlay}>\n                                        <h4 className={classes.recipyTitle} style={{top: calculateLength(recipy.titulo)}}>{recipy.titulo}</h4>\n                                    </div>\n                                \n                            </div>\n                        </Link>\n                        \n                    ))}\n                </div>\n            </div>\n            }\n\n            {/* Scroll */}\n            {recipes.length > 5 && \n                <p className={classes.scroll}>Scroll<ArrowRightAltIcon/></p>\n            }\n        </>\n    )\n}\n\nexport default RecetaRow\n","/Users/abrahamcepedaoseguera/Documents/dfuture/NUTZEN/react-website/nutzen/src/components/recetas/RecetaPost.js",[],"/Users/abrahamcepedaoseguera/Documents/dfuture/NUTZEN/react-website/nutzen/src/components/home/Phrase.js",["139"],"/Users/abrahamcepedaoseguera/Documents/dfuture/NUTZEN/react-website/nutzen/src/components/home/Stores.js",[],"/Users/abrahamcepedaoseguera/Documents/dfuture/NUTZEN/react-website/nutzen/src/resources/Colors.js",[],"/Users/abrahamcepedaoseguera/Documents/dfuture/NUTZEN/react-website/nutzen/src/components/admin/RecetaRow.js",[],"/Users/abrahamcepedaoseguera/Documents/dfuture/NUTZEN/react-